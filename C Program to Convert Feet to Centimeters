#include<stdio.h> 
//#include 

void main() {

  float cm;
  float feet;
  

//    clrscr();

	printf(" Program to convert Feet to Centimeter:  ");

	printf("\n\n Enter Length in Feet  : ");
	scanf("%f", &feet);
    
    cm = (feet/3.26)*100;
    
    printf("\n\n %f Feet in Meter = %f  \n",feet,cm);



//	getch();



}
